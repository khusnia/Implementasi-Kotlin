fun main() {
    /**
     * convert data type from integer to string or float and how to operating then
     */
​
    //deklarasi variable
    var angkaPertama : Int = 10
    var masukkanAngka = 13.0
​
    //konvert type data to int
    angkaPertama = masukkanAngka.toInt()
​
    //cetak hasil
    println(angkaPertama + 122)
}
