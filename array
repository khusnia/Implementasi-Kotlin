import java.util.*

fun main(args: Array<String>){
    val huruf = arrayOf("D","A","C","B","E")
    for (i in huruf.indices){
        Arrays.sort(huruf ,0,5)
        println("Ascending :"+huruf[i])
    }
    for (j in huruf.indices){
        Arrays.sort(huruf, Collections.reverseOrder())
        println(" Descending : "+huruf[j])
    }
}
